{
  "name": "toran-www",
  "version": "1.0.0",
  "description": "Toran WWW - Admin panel and API for Toran API Gateway",
  "private": true,
  "workspaces": [
    "www",
    "shared"
  ],
  "scripts": {
    "dev": "vercel dev",
    "build": "cd www && npm run build",
    "deploy": "vercel --prod",
    "setup:mongodb": "node scripts/setup-mongodb.js",
    "seed:data": "node scripts/seed-data.js",
    "setup:all": "npm run setup:mongodb && npm run seed:data"
  },
  "dependencies": {
    "@vercel/node": "^3.0.0",
    "dotenv": "^16.0.0",
    "ioredis": "^5.3.2",
    "mongodb": "^7.0.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "vercel": "^33.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
