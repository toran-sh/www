{
  "name": "toran-www",
  "version": "1.0.0",
  "description": "Toran WWW - Admin panel and API for Toran API Gateway",
  "private": true,
  "type": "module",
  "workspaces": [
    "shared"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "vercel --prod",
    "type-check": "tsc --noEmit",
    "setup:mongodb": "node scripts/setup-mongodb.js",
    "seed:data": "node scripts/seed-data.js",
    "setup:all": "npm run setup:mongodb && npm run seed:data"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "@tanstack/react-query": "^5.17.0",
    "@vercel/node": "^3.0.0",
    "clsx": "^2.0.0",
    "date-fns": "^3.0.0",
    "dotenv": "^16.0.0",
    "ioredis": "^5.3.2",
    "lucide-react": "^0.294.0",
    "mongodb": "^7.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.21.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "typescript": "^5.3.3",
    "vercel": "^33.0.0",
    "vite": "^7.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
